  keywords = {Asteroid Strength,Astrophysics - Earth and Planetary Astrophysics,Condensed Matter - Mesoscale and Nanoscale Physics,Condensed Matter - Soft Condensed Matter},
  file = {/Users/jrobinson/Zotero/storage/SAZGGBA6/Persson and Biele - 2021 - On the stability of spinning asteroids.pdf}
}

