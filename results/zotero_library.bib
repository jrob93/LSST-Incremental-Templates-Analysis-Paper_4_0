@article{ishiguroINTERPRETATION596SCHEILA2011,
  title = {{{INTERPRETATION OF}} (596) {{SCHEILA}}'{{S TRIPLE DUST TAILS}}},
  author = {Ishiguro, Masateru and Hanayama, Hidekazu and Hasegawa, Sunao and Sarugaku, Yuki and Watanabe, Jun-ichi and Fujiwara, Hideaki and Terada, Hiroshi and Hsieh, Henry H. and Vaubaillon, Jeremie J. and Kawai, Nobuyuki and Yanagisawa, Kenshi and Kuroda, Daisuke and Miyaji, Takeshi and Fukushima, Hideo and Ohta, Kouji and Hamanowa, Hiromi and Kim, Junhan and Pyo, Jeonghyun and Nakamura, Akiko M.},
