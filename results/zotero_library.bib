  abstract = {ABSTRACT             Capture of interstellar objects (ISOs) into the Solar system is dominated by ISOs with asymptotic incoming speeds \$v\_{\textbackslash}infty {\textbackslash}lt 4{\textbackslash},\$km\,s-1. The capture rate is proportional to the ISO phase-space density in the Solar vicinity and does not vary along the Sun's Galactic orbit, i.e. is not enhanced during a passage through a cloud of ISOs (in contrast to previous suggestions). Most bound orbits crossing those of Jupiter and Saturn are fully mixed with unbound phase space, implying that they hold the same ISO phase-space density. Assuming an interstellar number density of \$\{n\}\_\{{\textbackslash}mathrm\{iso\}\}{\textbackslash}sim 0.1{\textbackslash},\$au-3, we estimate that in 1000~years the planets capture {$\sim$}2 ISOs (while {$\sim$}17 fall into the Sun), resulting in a population of {$\sim$}8 captured ISOs within 5\,au of the Sun at any time, less than the number of visiting ISOs passing through the same volume on hyperbolic orbits. In terms of phase-space volume, capture into and ejection from the Solar system are equal, such that on average ISOs will not remain captive at \$a{\textbackslash}lesssim 2000{\textbackslash},\$au for extensive periods.},
  langid = {english},
  file = {/Users/jrobinson/Zotero/storage/PTGP9FRZ/Dehnen et al. - 2022 - Capture of interstellar objects – II. By the Solar.pdf}
}

@article{clementMathematicalEncodingMultiresonant2022,
  title = {Mathematical Encoding within Multi-Resonant Planetary Systems as {{SETI}} Beacons},
  author = {Clement, Matthew S. and Raymond, Sean N. and Veras, Dimitri and Kipping, David},
  year = {2022},
  month = apr,
  journal = {arXiv:2204.14259 [astro-ph, physics:physics]},
  eprint = {2204.14259},
  primaryclass = {astro-ph, physics:physics},
  urldate = {2022-05-12},
  abstract = {How might an advanced alien civilization manipulate the orbits within a planetary system to create a durable signpost that communicates its existence? While it is still debated whether such a purposeful advertisement would be prudent and wise, we propose that mean-motion resonances between neighboring planets -- with orbital periods that form integer ratios -- could in principle be used to encode simple sequences that one would not expect to form in nature. In this Letter we build four multi-resonant planetary systems and test their long-term orbital stability. The four systems each contain 6 or 7 planets and consist of: (i) consecutive integers from 1 to 6; (ii) prime numbers from 2 to 11; (iii) the Fibonacci sequence from 1 to 13; and (iv) the Lazy Caterer sequence from 1 to 16. We built each system using N-body simulations with artificial migration forces. We evaluated the stability of each system over the full 10 Gyr integration of the Sun's main sequence phase. We then tested the stability of these systems for an additional 10 Gyr, during and after post-main sequence evolution of the central stars (assumed to be Sun-like) to their final, white dwarf phase. The only system that was destabilized was the consecutive integer sequence (system i). The other three sequences therefore represent potential SETI beacons.},
  archiveprefix = {arXiv},
  keywords = {Aliens,Astrophysics - Earth and Planetary Astrophysics,N-Body,Physics - Popular Physics,Resonant Dynamics},
  file = {/Users/jrobinson/Zotero/storage/PR9IJ66S/Clement et al_2022_Mathematical encoding within multi-resonant planetary systems as SETI beacons.pdf;/Users/jrobinson/Zotero/storage/K7V32PZ8/2204.html}
}

@article{carreraStreamingInstabilityCannot2022,
  title = {The {{Streaming Instability Cannot Form Planetesimals}} from Mm-Size {{Grains}} in {{Pressure Bumps}}},
  author = {Carrera, Daniel and Simon, Jacob B.},
  year = {2022},
  month = apr,
  journal = {arXiv:2204.14270 [astro-ph]},
  eprint = {2204.14270},
  primaryclass = {astro-ph},
  urldate = {2022-05-12},
  abstract = {We present evidence that it is unlikely that the streaming instability (SI) can form planetesimals from mm grains inside axisymmetric pressure bumps. We conducted the largest simulation of the SI so far (7 million CPU hours), consisting of a large slice of the disk with mm grains, a solar-like dust-to-gas ratio (\$Z = 0.01\$), and the largest pressure bump that does not cause gravitational instability (GI) in the particle layer. We used a high resolution of \$1000/H\$ to resolve as many unstable modes of the SI as possible. The simulation produced a long-lived particle over-density far exceeding the SI criteria (i.e., a critical solid abundance to headwind parameter ratio \$Z/{\textbackslash}Pi\$) where past studies predict strong clumping; yet we observed none. The likely reason is that the time it takes particles to cross the high-\$Z/{\textbackslash}Pi\$ region (\$t\_\{{\textbackslash}rm cross\}\$) is shorter than the growth timescale of the SI (\$t\_\{{\textbackslash}rm grow\}\$). We propose an added criterion for planetesimal formation by the SI -- that \$t\_\{{\textbackslash}rm cross\} {$>$} t\_\{{\textbackslash}rm grow\}\$. We show that any bump larger than the one in this run would form planetesimals by the GI instead of the SI. Our results significantly restrict the pathways to planet formation: Either protoplanetary disks regularly form grains larger than 1{\textasciitilde}mm, or planetesimals do not form by the SI in axisymmetric pressure bumps. Since bumps large enough to induce the GI are likely Rossby-wave unstable, we propose that mm grains may only form planetesimals in vortices.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Numerical Methods,Planetesimal Formation & Growth,Streaming Instability},
  file = {/Users/jrobinson/Zotero/storage/I4IR5ZRT/Carrera_Simon_2022_The Streaming Instability Cannot Form Planetesimals from mm-size Grains in.pdf;/Users/jrobinson/Zotero/storage/TJWEBZZ9/2204.html}
}

@article{akhlaghiLongTermArchivableReproducibility2021,
  title = {Toward {{Long-Term}} and {{Archivable Reproducibility}}},
  author = {Akhlaghi, Mohammad and {Infante-Sainz}, Ra{\'u}l and Roukema, Boudewijn F. and Khellat, Mohammadreza and {Valls-Gabaud}, David and {Baena-Gall{\'e}}, Roberto},
