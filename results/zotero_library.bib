@article{fuksaAdvancedMultipoleModel2023,
  title = {An Advanced Multipole Model of the (130) {{Elektra}} Quadruple System},
  author = {Fuksa, M. and Bro{\v z}, M. and Hanu{\v s}, J. and Ferrais, M. and Fatka, P. and Vernazza, P.},
