@article{cukBarrelInstabilityBinary2021,
  title = {Barrel {{Instability}} in {{Binary Asteroids}}},
  author = {{\'C}uk, Matija and Jacobson, Seth A. and Walsh, Kevin J.},
  year = {2021},
