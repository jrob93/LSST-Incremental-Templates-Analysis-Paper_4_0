@article{tonryATLASHighcadenceAllsky2018,
  title = {{{ATLAS}}: {{A High-cadence All-sky Survey System}}},
  shorttitle = {{{ATLAS}}},
  author = {Tonry, J. L. and Denneau, L. and Heinze, A. N. and Stalder, B. and Smith, K. W. and Smartt, S. J. and Stubbs, C. W. and Weiland, H. J. and Rest, A.},
