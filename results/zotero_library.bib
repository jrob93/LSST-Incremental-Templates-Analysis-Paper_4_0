@article{boehnhardtVLTObservationsComet2002,
  title = {{{VLT}} Observations of Comet {{46P}}/{{Wirtanen}}},
  author = {Boehnhardt, H. and Delahodde, C. and Sekiguchi, T. and Tozzi, G. P. and Amestica, R. and Hainaut, O. and Spyromilio, J. and Tarenghi, M. and West, R. M. and Schulz, R. and Schwehm, G.},
  year = {2002},
