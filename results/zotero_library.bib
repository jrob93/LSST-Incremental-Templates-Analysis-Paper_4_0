@misc{gkotsinasEarlyThermalProcessing2024,
  title = {On the Early Thermal Processing of Planetesimals during and after the Giant Planet Instability},
  author = {Gkotsinas, A. and Nesvorny, D. and {Guilbert-Lepoutre}, A. and Raymond, S. N. and Kaib, N.},
