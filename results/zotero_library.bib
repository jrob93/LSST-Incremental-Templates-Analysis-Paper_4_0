@article{licandroObservationsTwoSuperfast2023,
  title = {Observations of Two Superfast Rotator {{NEAs}}: 2021 {{NY1}} and 2022 {{AB}}},
  shorttitle = {Observations of Two Superfast Rotator {{NEAs}}},
  author = {Licandro, Javier and Popescu, Marcel and Tatsumi, Eri and Alarcon, Miguel R and {Serra-Ricart}, Miquel and Medeiros, Hissa and Morate, David and {Tinaut-Ruano}, Fernando and {de~Le{\'o}n}, Julia},
  year = {2023},
  month = mar,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {521},
  number = {3},
  pages = {3784--3792},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/stad708},
  urldate = {2023-04-27},
  abstract = {ABSTRACT             In the framework of the visible NEAs observations survey (ViNOS) that uses several telescopes at the Canary Islands observatories since 2018, we observed two superfast rotator NEAs, 2021 NY1 and 2022 AB. We obtained photometry and spectrophotometry of both targets and visible spectroscopy of 2022 AB. Light curves of 2021 NY1 obtained in four different nights between 2021 September 30 and 2021 October 16 return a rotation period P~= 13.3449~{\textpm}~0.0013 minutes and a light curve amplitude A~= 1.00 mag. We found that 2021 NY1 is a very elongated superfast rotator with an axis ratio a/b {$\geq$} 3.6. We also report colours (g - r)~= 0.664~{\textpm}~0.013, (r - i)~= 0.186~{\textpm}~0.013, and (i - zs)~= -0.117~{\textpm}~0.012 mag. These are compatible with an S-type asteroid. The light curves of 2022 AB obtained on 2021 January 5 and 2021 January 8 show a rotation period P~= 3.0304~{\textpm}~0.0008 min, with amplitudes A~= 0.52 and A~= 0.54 mag. 2022 AB is also an elongated object with axis ratio a/b {$\geq$} 1.6. The obtained colours are (g - r)~= 0.400~{\textpm}~0.017, (r - i)~= 0.133~{\textpm}~0.017, and (i - zs)~= 0.093~{\textpm}~0.016. These colours are similar to those of the X-types, but with an unusually high (g - r) value. Spectra obtained on 2022 January 12 and 2022 January 14 are consistent with the reported colours. The spectral upturn over the 0.4 - 0.6 \${\textbackslash}mu {\textbackslash}mathrm\{m\}\$ region of 2022 AB does not fit with any known asteroid taxonomical class or meteorite spectrum, confirming its unusual surface properties.},
  langid = {english},
  keywords = {Light Curves,Near Earth Objects,Small Body Rotation},
  file = {/Users/jrobinson/Zotero/storage/WFF3MGEX/Licandro et al. - 2023 - Observations of two superfast rotator NEAs 2021 N.pdf}
}

