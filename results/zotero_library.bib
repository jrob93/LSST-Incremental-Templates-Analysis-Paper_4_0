  author = {Strauss, R. and Trilling, D. E. and Bernardinelli, P. H. and Beach, C. and Oldroyd, W. J. and Sheppard, S. S. and Schlichting, H. E. and Gerdes, D. W. and Adams, F. C. and Chandler, C. O. and Fuentes, C. and Holman, M. J. and Juri{\'c}, M. and Lin, H. W. and Markwardt, L. and McNeill, A. and Mommert, M. and Napier, K. J. and Payne, M. J. and Ragozzine, D. and Rivkin, A. S. and Smotherman, H. and Trujillo, C. A.},
  year = {2023},
