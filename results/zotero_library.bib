}

@article{siltalaMassesBulkDensities2021,
  title = {Masses, Bulk Densities, and Macroporosities of Asteroids (15) {{Eunomia}}, (29) {{Amphitrite}}, (52) {{Europa}}, and (445) {{Edna}} Based on {{Gaia}} Astrometry},
  author = {Siltala, Lauri and Granvik, Mikael},
  year = {2021},
