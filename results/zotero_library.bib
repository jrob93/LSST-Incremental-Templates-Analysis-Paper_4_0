  author = {Lee, H.-J. and Kim, M.-J. and Marciniak, A. and Kim, D.-H. and Moon, H.-K. and Choi, Y.-J. and Zo{\l}a, S. and Chatelain, J. and Lister, T. A. and Gomez, E. and Greenstreet, S. and P{\'a}l, A. and Szak{\'a}ts, R. and Erasmus, N. and Lees, R. and {van Rensburg}, P. Janse and Og{\l}oza, W. and Dr{\'o}{\.z}d{\.z}, M. and {\.Z}ejmo, M. and Kami{\'n}ski, K. and Kami{\'n}ska, M. K. and Duffard, R. and Roh, D.-G. and Yim, H.-S. and Kim, T. and Mottola, S. and Yoshida, F. and Reichart, D. E. and Sonbas, E. and Caton, D. B. and Kaplan, M. and Erece, O. and Yang, H.},
