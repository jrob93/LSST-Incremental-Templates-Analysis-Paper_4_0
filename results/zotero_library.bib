  author = {Popescu, Marcel M. and V{\u a}duvescu, O. and de Le{\'o}n, J. and Marcos, C. de la Fuente and Marcos, R. de la Fuente and St{\u a}nescu, M. O. and Alarcon, M. R. and Ricart, M. Serra and Licandro, J. and Berte{\c s}teanu, D. and Predatu, M. and Curelaru, L. and Barwell, F. and Jhass, K. and Boldea, C. and Mac{\'i}as, A. Aznar and Hudin, L. and Dumitru, B. A.},
