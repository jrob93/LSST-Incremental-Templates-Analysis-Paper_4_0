@article{nesvornyNEOMODNewOrbital2023,
  title = {{{NEOMOD}}: {{A New Orbital Distribution Model}} for {{Near-Earth Objects}}},
  shorttitle = {{{NEOMOD}}},
  author = {Nesvorn{\'y}, David and Deienno, Rogerio and Bottke, William F. and Jedicke, Robert and Naidu, Shantanu and Chesley, Steven R. and Chodas, Paul W. and Granvik, Mikael and Vokrouhlick{\'y}, David and Bro{\v z}, Miroslav and Morbidelli, Alessandro and Christensen, Eric and Shelly, Frank C. and Bolin, Bryce T.},
