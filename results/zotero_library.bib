@article{moeyensTHORAlgorithmCadenceindependent2021,
  title = {{{THOR}}: {{An Algorithm}} for {{Cadence-independent Asteroid Discovery}}},
  shorttitle = {{{THOR}}},
  author = {Moeyens, Joachim and Juri{\'c}, Mario and Ford, Jes and Bekte{\v s}evi{\'c}, Dino and Connolly, Andrew J. and Eggl, Siegfried and Ivezi{\'c}, {\v Z}eljko and Jones, R. Lynne and Kalmbach, J. Bryce and Smotherman, Hayden},
  year = {2021},
