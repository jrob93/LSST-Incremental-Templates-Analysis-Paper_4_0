@article{betzlerBVRPhotometryComets2020,
  title = {{{BVR}} Photometry of Comets {{63P}}/{{Wild}} 1 and {{C}}/2012 {{K1}} ({{PANSTARRS}})},
  author = {Betzler, A. S. and De Sousa, O. F. and Diepvens, A. and Bettio, T. M.},
