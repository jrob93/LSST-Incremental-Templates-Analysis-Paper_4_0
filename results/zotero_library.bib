@misc{jarmakEstimateWaterHydroxyl2024,
  title = {Estimate of Water and Hydroxyl Abundance on Asteroid (16) {{Psyche}} from {{JWST}} Data},
  author = {Jarmak, Stephanie G. and Becker, Tracy M. and Woodward, Charles E. and Honniball, Casey I. and Rivkin, Andrew S. and McAdam, Margaret M. and Landsman, Zoe A. and Cambioni, Saverio and M{\"u}ller, Thomas G. and Takir, Driss and Retherford, Kurt D. and Arredondo, Anicia and {Elkins-Tanton}, Linda T.},
  year = {2024},
