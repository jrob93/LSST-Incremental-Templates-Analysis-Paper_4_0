@misc{lammersIntrasystemUniformityNatural2023,
  title = {Intra-System Uniformity: A Natural Outcome of Dynamical Sculpting},
  shorttitle = {Intra-System Uniformity},
  author = {Lammers, Caleb and Murray, Norman and Hadden, Sam},
  year = {2023},
  month = apr,
  number = {arXiv:2304.02634},
  eprint = {2304.02634},
  primaryclass = {astro-ph},
  publisher = {arXiv},
  urldate = {2023-06-06},
  abstract = {There is evidence that exoplanet systems display intra-system uniformity in mass, radius, and orbital spacing (like "peas in a pod") when compared with the system-to-system variations of planetary systems. This has been interpreted as the outcome of the early stages of planet formation, indicative of a picture in which planets form at characteristic mass scales with uniform separations. In this paper, we argue instead that intra-system uniformity in planet sizes and orbital spacings likely arose from the long-term dynamical sculpting of initially-overly-packed planetary systems (in other words, the giant impact phase). With a suite of \$N\$-body simulations, we demonstrate that systems with random initial masses and compact planet spacings naturally develop intra-system uniformity, in quantitative agreement with observations, due to collisions between planets. Our results suggest that the pre-giant impact planet mass distribution is fairly wide and provide evidence for the prevalence of dynamical sculpting in shaping the observed population of exoplanets.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/EBLUWBN2/Lammers et al_2023_Intra-system uniformity.pdf;/Users/jrobinson/Zotero/storage/6FCAS86X/2304.html}
}

