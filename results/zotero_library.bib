@misc{mcdonnellPlanetFourNeural2022,
  title = {Planet {{Four}}: {{A Neural Network}}'s {{Search For Polar Spring-time Fans On Mars}}},
  shorttitle = {Planet {{Four}}},
  author = {McDonnell, Mark D. and Jones, Eriita and Schwamb, Megan E. and Aye, K.-Michael and Portyankina, Ganna and Hansen, Candice J.},
