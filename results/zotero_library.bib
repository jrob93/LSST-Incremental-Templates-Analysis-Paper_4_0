@article{crovisierSpectrumCometHaleBopp1997,
  title = {The {{Spectrum}} of {{Comet Hale-Bopp}} ({{C}}/1995 {{O1}}) {{Observed}} with the {{Infrared Space Observatory}} at 2.9 {{Astronomical Units}} from the {{Sun}}},
  author = {Crovisier, Jacques and Leech, Kieron and {Bockel{\'e}e-Morvan}, Dominique and Brooke, Timothy Y. and Hanner, Martha S. and Altieri, Bruno and Keller, H. Uwe and Lellouch, Emmanuel},
  year = {1997},
  month = mar,
  journal = {Science},
  volume = {275},
  number = {5308},
  pages = {1904--1907},
  publisher = {American Association for the Advancement of Science},
  doi = {10.1126/science.275.5308.1904},
  urldate = {2024-01-10},
  abstract = {Comet Hale-Bopp (C/1995 O1) was observed at wavelengths from 2.4 to 195 micrometers with the Infrared Space Observatory when the comet was about 2.9 astronomical units (AU) from the sun. The main observed volatiles that sublimated from the nucleus ices were water, carbon monoxide, and carbon dioxide in a ratio (by number) of 10:6:2. These species are also the main observed constituents of ices in dense interstellar molecular clouds; this observation strengthens the links between cometary and interstellar material. Several broad emission features observed in the 7- to 45-micrometer region suggest the presence of silicates, particularly magnesium-rich crystalline olivine. These features are similar to those observed in the dust envelopes of Vega-type stars.},
  file = {/Users/jrobinson/Zotero/storage/HW73UQXQ/Crovisier et al_1997_The Spectrum of Comet Hale-Bopp (C-1995 O1) Observed with the Infrared Space.pdf}
}

@article{crovisierSpectrumCometHaleBopp1999,
  title = {The {{Spectrum}} of {{Comet Hale-Bopp}} as Seen by {{ISO}}},
  author = {Crovisier, J. and Leech, K. and {Bockel{\'e}e-Morvan}, D. and Lellouch, E. and Brooke, T. Y. and Hanner, M. S. and Altieri, B. and Keller, H. U. and Lim, T.},
  year = {1999},
  month = mar,
  volume = {427},
  pages = {137},
  urldate = {2024-01-10},
  abstract = {Comet C/1995 O1 (Hale-Bopp) was observed with the Infrared Space Observatory within the target-of-opportunity comet programme. Spectra were obtained pre-perihelion on April 1996 and September-October 1996, and post-perihelion on December 1997 and April 1998, spanning heliocentric distances (r) between 2.8 and 4.9 AU. At the largest r's, only PHT-S spectra were obtained. At the smallest r's, a high-resolution spectrum covering the full 2.4 to 195 {$\mu$}m spectral range was obtained for the first time in a comet, using SWS and LWS. Spectra of dedicated regions (H2O, CO and CO2 bands) were also observed with SWS. The vibrational bands of water, carbon dioxide and carbon monoxide are detected in emission, as expected from molecular fluorescence models. Relative production rates for H2O:CO2:CO vary, as a function of r, as could be expected from the different volatilities of these species. H2O was observed at high spectral resolution in the {$\nu$}3 group of bands around 2.7 {$\mu$}m, the {$\nu$}2 group around 6 {$\mu$}m with SWS, and in several rotational lines with LWS. The ro-vibrational lines of the {$\nu$}3 band were observed with a high S/N in September-October 1996. This allows an accurate determination of the water rotational temperature (28 K) and of its ortho-to-para ratio (2.45 +/- 0.10), which significantly differs from the high temperature limit and corresponds to a spin temperature of 25 K). Longward of 6 {$\mu$}m, the spectrum is dominated by dust thermal continuum emission, upon which broad emission features are superimposed whose peak wavelengths correspond to those of Mg-rich crystalline olivine (forsterite). In the September-October spectra, emission features at 44 and 65 {$\mu$}m and possible absorption at 2.9-3.2 {$\mu$}m suggest that grains of water ice were present at 2.9 AU from the Sun.},
  keywords = {C/1995 O1 (HALE-BOPP),COMETS,DUST,MOLECULES,SPECTROSCOPY},
  annotation = {ADS Bibcode: 1999ESASP.427..137C},
  file = {/Users/jrobinson/Zotero/storage/CEPNMZE2/Crovisier et al_1999_The Spectrum of Comet Hale-Bopp as seen by ISO.pdf}
}

