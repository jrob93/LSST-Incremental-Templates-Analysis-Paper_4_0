  author = {Lejoly, C. and Harris, W. and Samarasinha, N. and Mueller, B. E. A. and Howell, E. and Bodnarik, J. and Springmann, A. and Kareta, T. and Sharkey, B. and Noonan, J. and Bedin, L. R. and Bosch, J.-G. and Brosio, A. and Bryssinck, E. and De Vanssay, J.-B. and Hambsch, F.-J. and Ivanova, O. and Krushinsky, V. and Lin, Z.-Y. and Manzini, F. and Maury, A. and Moriya, N. and Ochner, P. and Oldani, V.},
