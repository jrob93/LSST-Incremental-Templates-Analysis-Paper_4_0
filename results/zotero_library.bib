  shorttitle = {{{ASTEROID LIGHT CURVES FROM THE PALOMAR TRANSIENT FACTORY SURVEY}}},
  author = {Waszczak, Adam and Chang, Chan-Kao and Ofek, Eran O. and Laher, Russ and Masci, Frank and Levitan, David and Surace, Jason and Cheng, Yu-Chi and Ip, Wing-Huen and Kinoshita, Daisuke and Helou, George and Prince, Thomas A. and Kulkarni, Shrinivas},
  year = {2015},
  month = aug,
  journal = {The Astronomical Journal},
  volume = {150},
