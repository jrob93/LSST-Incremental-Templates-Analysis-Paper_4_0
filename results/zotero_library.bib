@article{opitomLongtermActivityOutburst2016,
  title = {Long-Term Activity and Outburst of Comet {{C}}/2013 {{A1}} ({{Siding Spring}}) from Narrow-Band Photometry and Long-Slit Spectroscopy},
  author = {Opitom, C. and {Guilbert-Lepoutre}, A. and Jehin, E. and Manfroid, J. and Hutsem{\'e}kers, D. and Gillon, M. and Magain, P. and {Roberts-Borsani}, G. and Witasse, O.},
