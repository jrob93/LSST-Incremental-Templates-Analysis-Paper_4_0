@article{korsunDistantActivityComet2014,
  title = {Distant Activity of {{Comet C}}/2002 {{VQ94}} ({{LINEAR}}): {{Optical}} Spectrophotometric Monitoring between 8.4 and 16.8 Au from the {{Sun}}},
  shorttitle = {Distant Activity of {{Comet C}}/2002 {{VQ94}} ({{LINEAR}})},
  author = {Korsun, Pavlo P. and Rousselot, Philippe and Kulyk, Irina V. and Afanasiev, Viktor L. and Ivanova, Oleksandra V.},
  year = {2014},
