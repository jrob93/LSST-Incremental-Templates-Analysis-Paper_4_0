  author = {Kecskem{\'e}thy, Vikt{\'o}ria and Kiss, Csaba and Szak{\'a}ts, R{\'o}bert and P{\'a}l, Andr{\'a}s and Szab{\'o}, Gyula M. and Moln{\'a}r, L{\'a}szl{\'o} and S{\'a}rneczky, Kriszti{\'a}n and Vink{\'o}, J{\'o}zsef and Szab{\'o}, R{\'o}bert and Marton, G{\'a}bor and {Farkas-Tak{\'a}cs}, Anik{\'o} and Kalup, Csilla E. and Kiss, L{\'a}szl{\'o} L.},
