@misc{xuDiurnalYarkovskyEffect2022,
  title = {The Diurnal {{Yarkovsky}} Effect of Irregularly Shaped Asteroids},
  author = {Xu, Yang-Bo and Zhou, Li-Yong and Hui, Hejiu and Li, Jian-Yang},
