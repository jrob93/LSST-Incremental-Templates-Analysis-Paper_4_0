  author = {Thomas, P. and A'Hearn, M. and Belton, M.J.S. and Brownlee, D. and Carcich, B. and Hermalyn, B. and Klaasen, K. and Sackett, S. and Schultz, P.H. and Veverka, J. and Bhaskaran, S. and Bodewits, D. and Chesley, S. and Clark, B. and Farnham, T. and Groussin, O. and Harris, A. and Kissel, J. and Li, J.-Y. and Meech, K. and Melosh, J. and Quick, A. and Richardson, J. and Sunshine, J. and Wellnitz, D.},
