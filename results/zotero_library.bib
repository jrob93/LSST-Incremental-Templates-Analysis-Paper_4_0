  abstract = {The Oort cloud is thought to be a reservoir of icy planetesimals and the source of long-period comets (LPCs) implanted from the outer Solar System during the time of giant planet formation. The abundance of rocky ice-free bodies is a key diagnostic of Solar System formation models as it can distinguish between ``massive" and ``depleted" proto-asteroid belt scenarios and thus disentangle competing planet formation models. Here we report a direct observation of a decimeter-sized (\${\textbackslash}sim2\$ kg) rocky meteoroid on a retrograde LPC orbit (\$e {\textbackslash}approx 1.0\$, i = \$121{\textasciicircum}\{{\textbackslash}circ\}\$). During its flight, it fragmented at dynamic pressures similar to fireballs dropping ordinary chondrite meteorites. A numerical ablation model fit produces bulk density and ablation properties also consistent with asteroidal meteoroids. We estimate the flux of rocky objects impacting Earth from the Oort cloud to be \$1.08{\textasciicircum}\{+2.81\}\_\{-0.95\} {\textbackslash}mathrm\{meteoroids/10{\textasciicircum}6 km{\textasciicircum}2/yr\}\$ to a mass limit of 10 g. This corresponds to an abundance of rocky meteoroids of \${\textbackslash}sim6{\textasciicircum}\{+13\}\_\{-5\}\${\textbackslash}\% of all objects originating in the Oort cloud and impacting Earth to these masses. Our result gives support to migration-based dynamical models of the formation of the Solar System which predict that significant rocky material is implanted in the Oort cloud, a result not explained by traditional Solar System formation models.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Meteors & Meteorites,Outer Solar System},
  file = {/Users/jrobinson/Zotero/storage/TQ3F4HU8/Vida et al_2022_Direct measurement of decimeter-sized rocky material in the Oort cloud.pdf;/Users/jrobinson/Zotero/storage/85ZMA9V6/2212.html}
}

@misc{guilbert-lepoutreGatewayCentaursJupiterfamily2022,
  title = {The Gateway from {{Centaurs}} to {{Jupiter-family Comets}}: Thermal and Dynamical Evolution},
  shorttitle = {The Gateway from {{Centaurs}} to {{Jupiter-family Comets}}},
