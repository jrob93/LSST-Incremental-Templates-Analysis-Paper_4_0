@article{mommertEXPLORENEOsVIIIDORMANT2015,
  title = {{{EXPLORENEOs}}. {{VIII}}. {{DORMANT SHORT-PERIOD COMETS IN THE NEAR-EARTH ASTEROID POPULATION}}},
  author = {Mommert, M. and Harris, A. W. and Mueller, M. and Hora, J. L. and Trilling, D. E. and Bottke, W. F. and Thomas, C. A. and Delbo, M. and Emery, J. P. and Fazio, G. and Smith, H. A.},
