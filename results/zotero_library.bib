  author = {Kiss, Csaba and M{\"u}ller, Thomas G. and {Farkas-Tak{\'a}cs}, Anik{\'o} and Mo{\'o}r, Attila and Protopapa, Silvia and Parker, Alex H. and {Santos-Sanz}, Pablo and Ortiz, Jose Luis and Holler, Bryan J. and Wong, Ian and Stansberry, John and {Fern{\'a}ndez-Valenzuela}, Estela and Glein, Christopher R. and Lellouch, Emmanuel and Vilenius, Esa and Kalup, Csilla E. and Reg{\'a}ly, Zsolt and Szak{\'a}ts, R{\'o}bert and Marton, G{\'a}bor and P{\'a}l, Andr{\'a}s and Szab{\'o}, Gyula M.},
