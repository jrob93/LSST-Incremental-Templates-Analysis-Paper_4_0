@misc{visserProgradeSpinupGravitational2022,
  title = {Prograde Spin-up during Gravitational Collapse},
  author = {Visser, Rico G. and Brouwers, Marc G.},
  year = {2022},
  month = may,
  number = {arXiv:2205.09748},
  eprint = {2205.09748},
  primaryclass = {astro-ph, physics:physics},
  institution = {arXiv},
  urldate = {2022-05-23},
  abstract = {Asteroids, planets, stars in some open clusters, as well as molecular clouds appear to possess a preferential spin-orbit alignment, pointing to shared processes that tie their rotation at birth to larger parent structures. We present a new mechanism that describes how collections of particles or 'clouds' gain a prograde rotational component when they collapse or contract while subject to an external, central force. The effect is geometric in origin, as relative shear on curved orbits moves their shared center-of-mass slightly inward and toward the external potential during a collapse, exchanging orbital angular momentum into aligned (prograde) rotation. We perform illustrative analytical and N-body calculations to show that this process of prograde spin-up proceeds quadratically in time (\${\textbackslash}delta L\_{\textbackslash}mathrm\{rot\} {\textbackslash}propto t{\textasciicircum}2\$) until the collapse nears completion. The total rotational gain increases with the size of the cloud prior to its collapse: \${\textbackslash}delta L\_{\textbackslash}mathrm\{rot\}/L\_{\textbackslash}mathrm\{H\} {\textbackslash}propto (R\_{\textbackslash}mathrm\{cl\}/R\_{\textbackslash}mathrm\{H\}){\textasciicircum}5\$, and typically with distance to the source of the potential (\$L\_{\textbackslash}mathrm\{H\}{\textbackslash}propto r\_0)\$. For clouds that form at the interface of shear and self-gravity (\$R\_{\textbackslash}mathrm\{cl\} {\textbackslash}sim R\_{\textbackslash}mathrm\{H\}\$), prograde spin-up means that even setups with large initial retrograde rotation collapse to form prograde-spinning objects. Being a geometric effect, prograde spin-up persists around any central potential that triggers shear, even those where the shear is strongly retrograde. We highlight an application to the Solar System, where prograde spin-up can explain the frequency of binary objects in the Kuiper belt with prograde rotation.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Earth and Planetary Astrophysics,Astrophysics - Solar and Stellar Astrophysics,Binary Planetesimals,Gravitational Collapse,Physics - Classical Physics,Physics - Space Physics,Planetesimal Formation & Growth,Streaming Instability},
  file = {/Users/jrobinson/Zotero/storage/F47VITYX/Visser_Brouwers_2022_Prograde spin-up during gravitational collapse.pdf;/Users/jrobinson/Zotero/storage/6LHZY5XH/2205.html}
}

@misc{rivkinNatureLowAlbedoSmall2022,
  title = {The {{Nature}} of {{Low-Albedo Small Bodies}} from 3-\${\textbackslash}mu\$m {{Spectroscopy}}: {{One Group}} That {{Formed Within}} the {{Ammonia Snow Line}} and {{One}} That {{Formed Beyond It}}},
  shorttitle = {The {{Nature}} of {{Low-Albedo Small Bodies}} from 3-\${\textbackslash}mu\$m {{Spectroscopy}}},
  author = {Rivkin, Andrew S. and Emery, Joshua P. and Howell, Ellen S. and Kareta, Theodore and Noonan, John W. and Richardson, Matthew and Sharkey, Benjamin N. L. and Sickafoose, Amanda A. and Woodney, Laura M. and Cartwright, Richard J. and Lindsay, Sean and Mcclure, Lucas T.},
  year = {2022},
  month = may,
  number = {arXiv:2205.09166},
  eprint = {2205.09166},
  primaryclass = {astro-ph},
  institution = {arXiv},
  urldate = {2022-05-23},
  abstract = {We present evidence, via a large survey of 191 new spectra along with previously-published spectra, of a divide in the 3-\${\textbackslash}mu\$m spectral properties of the low-albedo asteroid population. One group ("Sharp-types" or ST, with band centers \${$<\$$} 3 \${\textbackslash}mu\$m) has a spectral shape consistent with carbonaceous chondrite meteorites, while the other group ("not-Sharp-types" or NST, with bands centered \${$>\$$} 3 \${\textbackslash}mu\$m) is not represented in the meteorite literature but is as abundant as the STs among large objects. Both groups are present in most low-albedo asteroid taxonomic classes, and except in limited cases taxonomic classifications based on 0.5-2.5-\${\textbackslash}mu\$m data alone cannot predict whether an asteroid is ST or NST. Statistical tests show the STs and NSTs differ in average band depth, semi-major axis, and perihelion at confidence levels \${\textbackslash}ge\$98{\textbackslash}\%, while not showing significant differences in albedo. We also show that many NSTs have a 3-\${\textbackslash}mu\$m absorption band shape like Comet 67P, and likely represent an important small-body composition throughout the solar system. A simple explanation for the origin of these groups is formation on opposite sides of the ammonia snow line, with the NST group accreting H2O and NH3 and the ST group only accreting H2O, with subsequent thermal and chemical evolution resulting in the minerals seen today. Such an explanation is consistent with recent dynamical modeling of planetesimal formation and delivery, and suggests that much more outer solar system material was delivered to the main asteroid belt than would be thought based on the number of D-class asteroids found today.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Small Body Composition,Spectroscopy},
  file = {/Users/jrobinson/Zotero/storage/U5IJCZZQ/Rivkin et al_2022_The Nature of Low-Albedo Small Bodies from 3-$-mu$m Spectroscopy.pdf;/Users/jrobinson/Zotero/storage/BATZMPIX/2205.html}
}

