@article{ansdellREFINEDROTATIONALPERIOD2014,
  title = {{{REFINED ROTATIONAL PERIOD}}, {{POLE SOLUTION}}, {{AND SHAPE MODEL FOR}} (3200) {{PHAETHON}}},
  author = {Ansdell, Megan and Meech, Karen J. and Hainaut, Olivier and Buie, Marc W. and Kaluna, Heather and Bauer, James and Dundon, Luke},
  year = {2014},
