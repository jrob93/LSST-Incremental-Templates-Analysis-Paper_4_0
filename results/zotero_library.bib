  file = {/Users/jrobinson/Zotero/storage/4UQECZIM/Bonev et al. - 2021 - First Comet Observations with NIRSPEC-2 at Keck O.pdf}
}

@article{rothProbingEvolutionaryHistory2020,
  title = {Probing the {{Evolutionary History}} of {{Comets}}: {{An Investigation}} of the {{Hypervolatiles CO}}, {{CH}} {\textsubscript{4}} , and {{C}} {\textsubscript{2}} {{H}} {\textsubscript{6}} in the {{Jupiter-family Comet 21P}}/{{Giacobini}}--{{Zinner}}},
