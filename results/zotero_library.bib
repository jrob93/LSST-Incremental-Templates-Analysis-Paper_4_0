  volume = {516},
  number = {1},
  eprint = {2208.11912},
  primaryclass = {astro-ph},
  pages = {L53-L57},
  issn = {1745-3925, 1745-3933},
  doi = {10.1093/mnrasl/slac072},
  urldate = {2022-10-08},
  abstract = {We report on the first polarimetric study of (3200) Phaethon, the target of JAXA's DESTINY\${\textasciicircum}+\$ mission, in the negative branch to ensure its anhydrous nature and to derive an accurate geometric albedo. We conducted observations at low phase angles (Sun-target-observer angle, alpha = 8.8-32.4 deg) from 2021 October to 2022 January and found that Phaethon has a minimum polarization degree \$P\_\{min\}\$ = -1.3 +- 0.1 \%, a polarimetric slope h = 0.22 +- 0.02 \% deg\${\textasciicircum}\{-1\}\$, and an inversion angle alpha\$\_0\$ = 19.9 +- 0.3 deg. The derived geometric albedo is \$p\_V\$ = 0.11 (in the range of 0.08-0.13). These polarimetric properties are consistent with anhydrous chondrites, and contradict hydrous chondrites and typical cometary nuclei.},
  archiveprefix = {arXiv},
  keywords = {Asteroid Polarimetry,Astrophysics - Earth and Planetary Astrophysics,Small Body Composition},
  file = {/Users/jrobinson/Zotero/storage/Q56DG3S8/Geem et al_2022_(3200) Phaethon Polarimetry in the Negative Branch.pdf;/Users/jrobinson/Zotero/storage/28FBTYG4/2208.html}
}

