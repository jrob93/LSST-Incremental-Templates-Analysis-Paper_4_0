@article{tancrediCatalogObservedNuclearmagnitudes2000,
  title = {A Catalog of Observed Nuclearmagnitudes of {{Jupiter}} Family Comets},
  author = {Tancredi, G. and Fern{\'a}ndez, J. A. and Rickman, H. and Licandro, J.},
