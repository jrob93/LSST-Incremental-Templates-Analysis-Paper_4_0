  keywords = {Astrophysics - Earth and Planetary Astrophysics,Dust and Pebbles,Physics - Geophysics},
  file = {/Users/jrobinson/Zotero/storage/BDZ33GSW/Kaneko et al. - 2021 - Dependence of the initial internal structure of ch.pdf}
}

