  keywords = {Astrophysics - Earth and Planetary Astrophysics,Astrophysics - Solar and Stellar Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/QXXIUSKR/Lisse et al_2022_A Predicted Dearth of Bulk Hypervolatile Ices in Oort Cloud Comets.pdf;/Users/jrobinson/Zotero/storage/87BA8S8P/2203.html}
}

@article{maclennanThermophysicalInvestigationAsteroid2022,
  title = {Thermophysical {{Investigation}} of {{Asteroid Surfaces II}}: {{Factors Influencing Grain Size}}},
