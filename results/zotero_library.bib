@article{yeComet2021HS2023,
  title = {Comet {{P}}/2021 {{HS}} ({{PANSTARRS}}) and the {{Challenge}} of {{Detecting Low-activity Comets}}},
  author = {Ye, Quanzhi and Kelley, Michael S. P. and Bauer, James M. and Farnham, Tony L. and Bodewits, Dennis and Buzzi, Luca and Weryk, Robert and Masci, Frank J. and Medford, Michael S. and Riddle, Reed and Wold, Avery},
  year = {2023},
  month = mar,
  journal = {The Planetary Science Journal},
  volume = {4},
