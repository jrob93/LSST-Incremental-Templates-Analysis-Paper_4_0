@article{huiLingeringDeathPeriodic2022,
  title = {The {{Lingering Death}} of {{Periodic Near-Sun Comet 323P}}/{{SOHO}}},
  author = {Hui, Man-To and Tholen, David J. and Kracht, Rainer and Chang, Chan-Kao and Wiegert, Paul A. and Ye, Quan-Zhi and Mutchler, Max},
  year = {2022},
  month = mar,
  journal = {arXiv:2203.02999 [astro-ph]},
  eprint = {2203.02999},
  primaryclass = {astro-ph},
  urldate = {2022-03-30},
  abstract = {We observed near-Sun comet 323P/SOHO for the first time using ground and space telescopes. In late December 2020, the object was recovered at Subaru showing no cometary features on its way to perihelion. However, in our postperihelion observations it developed a long narrow tail mimicking a disintegrating cometary debris cloud. The ejecta, comprised of at least mm-sized dust with power-law size distribution index \$3.2 {\textbackslash}pm 0.2\$, was impulsively produced shortly after the perihelion passage, during which \${\textbackslash}gtrsim\$0.1-10\% of the nucleus mass was shed due to excessive thermal stress and rotational disruption. Two fragments of \${\textbackslash}sim\$20 m in radius (assuming a geometric albedo of 0.15) were seen in HST observations from early March 2021. The nucleus, with an effective radius of \$86 {\textbackslash}pm 3\$ m (the same albedo assumed) and an aspect ratio of \${\textbackslash}lesssim\$0.7, has a rotation period of 0.522 hr, which is the shortest for known comets in the solar system and implies cohesive strength \${\textbackslash}gtrsim\$10-100 Pa in the interior. The colour of the object was peculiar, and how it changed temporally has never been previously observed. Using our astrometry, we found a strong nongravitational effect following a heliocentric dependency of \$r\_\{{\textbackslash}rm H\}{\textasciicircum}\{-8.5\}\$ in the transverse motion of the object. Our N-body integration reveals that 323P has a likelihood of 99.7\% to collide with the Sun in the next two millennia driven by the \${\textbackslash}nu\_6\$ secular resonance.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/NQ7VYL3V/Hui et al_2022_The Lingering Death of Periodic Near-Sun Comet 323P-SOHO.pdf;/Users/jrobinson/Zotero/storage/LMXRE97G/2203.html}
}

