@misc{fraserTransitionKuiperBelt2022,
  title = {The {{Transition}} from the {{Kuiper Belt}} to the {{Jupiter-Family}} ({{Comets}})},
  author = {Fraser, Wesley C. and Dones, Luke and Volk, Kathryn and Womack, Maria and Nesvorn{\'y}, David},
