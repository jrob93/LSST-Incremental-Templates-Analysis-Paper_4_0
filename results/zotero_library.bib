@misc{bottkeCollisionalEvolutionPrimordial2023,
  title = {The {{Collisional Evolution}} of the {{Primordial Kuiper Belt}}, {{Its Destabilized Population}}, and the {{Trojan Asteroids}}},
  author = {Bottke, William and Vokrouhlicky, David and Marshall, Raphael and Nesvorny, David and Morbidelli, Alessandro and Deienno, Rogerio and Marchi, Simone and Dones, Luke and Levison, Harold},
