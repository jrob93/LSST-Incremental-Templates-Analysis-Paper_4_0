@article{rozekPhysicalPropertiesNearEarth2022,
  title = {Physical Properties of Near-{{Earth}} Asteroid (2102) {{Tantalus}} from Multi-Wavelength Observations},
  author = {Ro{\.z}ek, Agata and Lowry, Stephen C. and Rozitis, Benjamin and Dover, Lord R. and Taylor, Patrick A. and Virkki, Anne and Green, Simon F. and Snodgrass, Colin and Fitzsimmons, Alan and {Campbell-White}, Justyn and Sajadian, Sedighe and Bozza, Valerio and Burgdorf, Martin J. and Dominik, Martin and Jaimes, R. Figuera and Hinse, Tobias C. and Hundertmark, Markus and J{\o}rgensen, Uffe G. and {Longa-Pe{\~n}a}, Pen{\'e}lope and Rabus, Markus and Rahvar, Sohrab and Skottfelt, Jesper and Southworth, John},
  year = {2022},
  month = jul,
  journal = {Monthly Notices of the Royal Astronomical Society},
  eprint = {2206.14306},
  primaryclass = {astro-ph},
  pages = {stac1835},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/stac1835},
  urldate = {2022-07-25},
  abstract = {Between 2010 and 2017 we have collected new optical and radar observations of the potentially hazardous asteroid (2102){\textasciitilde}Tantalus from the ESO NTT and Danish telescopes at the La Silla Observatory and from the Arecibo planetary radar. The object appears to be nearly spherical, showing a low amplitude light-curve variation and limited large-scale features in the radar images. The spin-state is difficult to constrain with the available data; including a certain light-curve subset significantly changes the spin-state estimates, and the uncertainties on period determination are significant. Constraining any change in rotation rate was not possible, despite decades of observations. The convex lightcurve-inversion model, with rotational pole at \$\{{\textbackslash}lambda\}=210\{{\textbackslash}pm\}41\$\{{\textbackslash}deg\} and \$\{{\textbackslash}beta\}=-30\{{\textbackslash}pm\}35\$\{{\textbackslash}deg\}, is more flattened than the two models reconstructed by including radar observations: with prograde (\$\{{\textbackslash}lambda\}=36\{{\textbackslash}pm\}23\$\{{\textbackslash}deg\}, \$\{{\textbackslash}beta\}=30\{{\textbackslash}pm\}15\$\{{\textbackslash}deg\}), and with retrograde rotation mode (\$\{{\textbackslash}lambda\}=180\{{\textbackslash}pm\}24\$\{{\textbackslash}deg\}, \$\{{\textbackslash}beta\}=-30\{{\textbackslash}pm\}16\$\{{\textbackslash}deg\}). Using data from WISE we were able to determine that the prograde model produces the best agreement in size determination between radar and thermophysical modelling. Radar measurements indicate possible variation in surface properties, suggesting one side might have lower radar albedo and be rougher at centimetre-to-decimetre scale than the other. However, further observations are needed to confirm this. Thermophysical analysis indicates a surface covered in fine-grained regolith, consistent with radar albedo and polarisation ratio measurements. Finally, geophysical investigation of the spin-stability of Tantalus shows that it could be exceeding its critical spin-rate via cohesive forces.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/65VM7A7Q/Rożek et al_2022_Physical properties of near-Earth asteroid (2102) Tantalus from.pdf;/Users/jrobinson/Zotero/storage/WDF69TNT/2206.html}
}

@misc{kelleyLOOKOutburstsComet2022,
  title = {A {{LOOK}} at {{Outbursts}} of {{Comet C}}/2014 {{UN}}\$\_\{271\}\$ ({{Bernardinelli-Bernstein}}) {{Near}} 20 Au},
  author = {Kelley, Michael S. P. and Kokotanekova, Rosita and Holt, Carrie E. and Protopapa, Silvia and Bodewits, Dennis and Knight, Matthew M. and Lister, Tim and Usher, Helen and Chatelain, Joseph and Gomez, Edward and Greenstreet, Sarah and Angel, Tony and Wooding, Ben},
  year = {2022},
  month = jun,
  number = {arXiv:2206.14888},
  eprint = {2206.14888},
  primaryclass = {astro-ph},
  publisher = {arXiv},
  urldate = {2022-07-26},
  abstract = {Cometary activity may be driven by ices with very low sublimation temperatures, such as carbon monoxide ice, which can sublimate at distances well beyond 20 au. This point is emphasized by the discovery of Oort cloud comet C/2014 UN\$\_\{271\}\$ (Bernardinelli-Bernstein), and its observed activity out to \${\textbackslash}sim\$26 au. Through observations of this comet's optical brightness and behavior, we can potentially discern the drivers of activity in the outer solar system. We present a study of the activity of comet Bernardinelli-Bernstein with broad-band optical photometry taken at 19-20 au from the Sun (2021 June to 2022 February) as part of the LCO Outbursting Objects Key (LOOK) Project. Our analysis shows that the comet's optical brightness during this period was initially dominated by cometary outbursts, stochastic events that ejected \${\textbackslash}sim10{\textasciicircum}7\$ to \${\textbackslash}sim10{\textasciicircum}8\$ kg of material on short ({$<$} 1 day) timescales. We present evidence for three such outbursts occurring in 2021 June and September. The nominal nuclear volumes excavated by these events are similar to the 10-100 m pit-shaped voids on the surfaces of short-period comet nuclei, as imaged by spacecraft. Two out of three Oort cloud comets observed at large pre-perihelion distances exhibit outburst behavior near 20 au, suggesting such events may be common in this population. In addition, quiescent CO-driven activity may account for the brightness of the comet in 2022 January to February, but that variations in the cometary active area (i.e., the amount of sublimating ice) with heliocentric distance are also possible.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Comets,Outer Solar System,Small Body Composition},
  file = {/Users/jrobinson/Zotero/storage/L2F7UYKJ/Kelley et al_2022_A LOOK at Outbursts of Comet C-2014 UN$_ 271 $ (Bernardinelli-Bernstein) Near.pdf;/Users/jrobinson/Zotero/storage/8SQ9RM33/2206.html}
}

@article{morenoGroundbasedObservabilityDimorphos2022,
  title = {Ground-Based Observability of {{Dimorphos DART}} Impact Ejecta: {{Photometric}} Predictions},
  shorttitle = {Ground-Based Observability of {{Dimorphos DART}} Impact Ejecta},
  author = {Moreno, Fernando and Bagatin, Adriano Campo and Tancredi, Gonzalo and Liu, Po-Yen and Dominguez, Bruno},
  year = {2022},
  month = jul,
  journal = {Monthly Notices of the Royal Astronomical Society},
  eprint = {2206.15350},
  primaryclass = {astro-ph},
  pages = {stac1849},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/stac1849},
  urldate = {2022-07-26},
  abstract = {The Double Asteroid Redirection Test (DART) is a NASA mission intended to crash a projectile on Dimorphos, the secondary component of the binary (65803) Didymos system, to study its orbit deflection. As a consequence of the impact, a dust cloud will be be ejected from the body, potentially forming a transient coma- or comet-like tail on the hours or days following the impact, which might be observed using ground-based instrumentation. Based on the mass and speed of the impactor, and using known scaling laws, the total mass ejected can be roughly estimated. Then, with the aim to provide approximate expected brightness levels of the coma and tail extent and morphology, we have propagated the orbits of the particles ejected by integrating their equation of motion, and have used a Monte Carlo approach to study the evolution of the coma and tail brightness. For typical power-law particle size distribution of index --3.5, with radii r\$\_\{rmin\}\$=1 \${\textbackslash}mu\$m and r\$\_\{max\}\$=1 cm, and ejection speeds near 10 times the escape velocity of Dimorphos, we predict an increase of brightness of \${\textbackslash}sim\$3 magnitudes right after the impact, and a decay to pre-impact levels some 10 days after. That would be the case if the prevailing ejection mechanism comes from the impact-induced seismic wave. However, if most of the ejecta is released at speeds of the order of \${\textbackslash}gtrsim\$100 \${\textbackslash}mathrm\{m{\textbackslash}; s{\textasciicircum}\{-1\}\}\$, the observability of the event would reduce to a very short time span, of the order of one day or shorter.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/DX26FYRH/Moreno et al_2022_Ground-based observability of Dimorphos DART impact ejecta.pdf;/Users/jrobinson/Zotero/storage/AU7GBVF2/2206.html}
}

@misc{lambertCitizenScienceAstronomy2022,
  title = {Citizen {{Science Astronomy}} with a {{Network}} of {{Small Telescope}}: {{The Launch}} and {{Deployment}} of {{JWST}}},
  shorttitle = {Citizen {{Science Astronomy}} with a {{Network}} of {{Small Telescope}}},
  author = {Lambert, R. A. and Marchis, F. and F and Asencio, J. and Blaclard, G. and Sgro, L. A. and Giorgini, J. D. and Plavchan, P. and White, T. and Verveen, A. and Goto, T. and Kuossari, P. and Sethu, N. and Loose, M. A. and Will, S. and Sibbernsen, K. and Pickering, J. W. and Randolph, J. and Fukui, K. and Huet, P. and Guillet, B. and Clerget, O. and Stahl, S. and Yoblonsky, N. and Lauvernier, M. and Matsumura, T. and Yamato, M. and Laugier, J. M. and {Brodt-Vilain}, O. and Espudo, A. and Kukita, R. and Iida, S. and Kardel, S. and Green, D. and Tikkanen, P. and Douvas, A. and Billiani, M. and Knight, G. and Ryno, M. and Simard, G. and Knight, R. and Primm, M. and Wildhagen, B. and Poncet, J. and Frachon, T. and Shimizu, M. and Jackson, A. and Parker, B. and Redfern, G. and Nikiforov, P. and Friday, E. and Lincoln, K. and Sweitzer, J. and Mitsuoka, R. and Cabral, K. and Katterfeld, A. and Fairfax, M.},
  year = {2022},
  month = jul,
  number = {arXiv:2207.04337},
  eprint = {2207.04337},
  primaryclass = {astro-ph},
  publisher = {arXiv},
  urldate = {2022-07-27},
  abstract = {We present a coordinated campaign of observations to monitor the brightness of the James Webb Space Telescope (JWST) as it travels toward the second Earth-Sun Lagrange point and unfolds using the network ofUnistellar digital telescopes. Those observations collected by citizen astronomers across the world allowed us to detect specific phases such as the separation from the booster, glare due to a change of orientation after a maneuver, the unfurling of the sunshield, and deployment of the primary mirror. After deployment of the sunshield on January 6 2022, the 6-h lightcurve has a significant amplitude and shows small variations due to the artificial rotation of the space telescope during commissionning. These variations could be due to the deployment of the primary mirror or some changes in orientation of the space telescope. This work illustrates the power of a worldwide array of small telescopes, operated by citizen astronomers, to conduct large scientific campaigns over a long timeframe. In the future, our network and others will continue to monitor JWST to detect potential degradations to the space environment by comparing the evolution of the lightcurve.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Astrophysics - Instrumentation and Methods for Astrophysics},
  file = {/Users/jrobinson/Zotero/storage/ZJN2AY9T/Lambert et al_2022_Citizen Science Astronomy with a Network of Small Telescope.pdf;/Users/jrobinson/Zotero/storage/HBSHPHEZ/2207.html}
}

@misc{devillepoixMinimoonStillLoose2022,
  title = {Minimoon Still on the Loose},
  author = {Devillepoix, Hadrien A. R. and Anderson, Seamus and Towner, Martin C. and Shober, Patrick M. and Jull, Anthony J. T. and Laubenstein, Matthias and Sansom, Eleanor K. and Bland, Philip A. and Cup{\'a}k, Martin and Howie, Robert M. and Hartig, Benjamin A. D. and Newsam, Garry N.},
  year = {2022},
