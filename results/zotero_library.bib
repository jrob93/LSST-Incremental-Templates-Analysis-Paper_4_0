@article{agrusaDynamicalEvolutionDidymosDimorphos2022,
  title = {Dynamical {{Evolution}} of the {{Didymos-Dimorphos Binary Asteroid}} as {{Rubble Piles}} Following the {{DART Impact}}},
  author = {Agrusa, Harrison F. and Ferrari, Fabio and Zhang, Yun and Richardson, Derek C. and Michel, Patrick},
  year = {2022},
  month = jul,
  journal = {The Planetary Science Journal},
