@article{emeryNEARINFRAREDSPECTROSCOPYTROJAN2011,
  title = {{{NEAR-INFRARED SPECTROSCOPY OF TROJAN ASTEROIDS}}: {{EVIDENCE FOR TWO COMPOSITIONAL GROUPS}}},
  shorttitle = {{{NEAR-INFRARED SPECTROSCOPY OF TROJAN ASTEROIDS}}},
  author = {Emery, J. P. and Burr, D. M. and Cruikshank, D. P.},
  year = {2011},
