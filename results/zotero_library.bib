  author = {Jones, R. L. and Yoachim, Peter and Chandrasekharan, Srinivasan and Connolly, Andrew J. and Cook, Kem H. and Ivezic, {\v Z}eljko and Krughoff, K. S. and Petry, Catherine and Ridgway, Stephen T.},
