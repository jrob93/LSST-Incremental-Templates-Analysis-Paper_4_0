  keywords = {Asteroid Colours,Astrophysics - Earth and Planetary Astrophysics,Outer Solar System,Planetary Migration,Small Body Composition},
  file = {/Users/jrobinson/Zotero/storage/AJS253YR/Marsset et al_2022_Col-OSSOS.pdf;/Users/jrobinson/Zotero/storage/DPCHA8UE/2206.html}
}

@misc{fraserColOSSOSTwoTypes2022,
  title = {Col-{{OSSOS}}: {{The Two Types}} of {{Kuiper Belt Surfaces}}},
  shorttitle = {Col-{{OSSOS}}},
  author = {Fraser, Wesley C. and Marsset, Rosemary E. Pike Michael and Schwamb, Megan E. and Bannister, Michele T. and Buchanan, Laura and Kavelaars, J. J. and Benecchi, Susan D. and Tan, Nicole J. and Peixinho, Nuno and Gwyn, Stephen D. J. and Chen, Ying-Tung and Gladman, Brett and Volk, Kathryn},
  year = {2022},
  month = jun,
  number = {arXiv:2206.04068},
  eprint = {2206.04068},
  primaryclass = {astro-ph},
  publisher = {arXiv},
  urldate = {2022-07-18},
  abstract = {The Colours of the Outer Solar System Origins Survey (Col-OSSOS) has gathered high quality, near-simultaneous (g-r) and (r-J) colours of 92 Kuiper Belt Objects (KBOs) with (u-g) and (r-z) gathered for some. We present the current state of the survey and data analysis. Recognizing that the optical colours of most icy bodies broadly follow the reddening curve, we present a new projection of the optical-NIR colours, which rectifies the main non-linear features in the optical-NIR along the ordinates. We find evidence for a bifurcation in the projected colours which presents itself as a diagonal empty region in the optical-NIR. A reanalysis of past colour surveys reveals the same bifurcation. We interpret this as evidence for two separate surface classes: the BrightIR class spans the full range of optical colours and broadly follows the reddening curve, while the FaintIR objects are limited in optical colour, and are less bright in the NIR than the BrightIR objects. We present a two class model. Objects in each class consist of a mix of separate blue and red materials, and span a broad range in colour. Spectra are modelled as linear optical and NIR spectra with different slopes, that intersect at some transition wavelength. The underlying spectral properties of the two classes fully reproduce the observed structures in the UV-optical-NIR colour space (\$0.4{\textbackslash}lesssim{\textbackslash}lambda{\textbackslash}lesssim1.4 {\textbackslash}mbox\{ \${\textbackslash}mu\$m\}\$), including the bifurcation observed in the Col-OSSOS and H/WTSOSS datasets, the tendency for cold classical KBOs to have lower (r-z) colours than excited objects, and the well known bimodal optical colour distribution.},
  archiveprefix = {arXiv},
  keywords = {Asteroid Colours,Astrophysics - Earth and Planetary Astrophysics,Outer Solar System,Small Body Composition},
  file = {/Users/jrobinson/Zotero/storage/SSXCDBF3/Fraser et al_2022_Col-OSSOS.pdf;/Users/jrobinson/Zotero/storage/BSYCPPHS/2206.html}
}

@misc{pikeColOSSOSDistributionSurface2022,
  title = {Col-{{OSSOS}}: {{The Distribution}} of {{Surface Classes}} in {{Neptune}}'s {{Resonances}}},
  shorttitle = {Col-{{OSSOS}}},
  author = {Pike, Rosemary E. and Fraser, Wesley C. and Volk, Kathryn and Kavelaars, J. J. and Marsset, Michael and Peixinho, Nuno and Schwamb, Megan E. and Bannister, Michele T. and Buchanan, Laura E. and Benecchi, Susan and Tan, Nicole},
  year = {2022},
