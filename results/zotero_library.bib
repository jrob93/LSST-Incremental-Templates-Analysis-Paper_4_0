  author = {Ootsubo, Takafumi and Kawakita, Hideyo and Hamada, Saki and Kobayashi, Hitomi and Yamaguchi, Mitsuru and Usui, Fumihiko and Nakagawa, Takao and Ueno, Munetaka and Ishiguro, Masateru and Sekiguchi, Tomohiko and Watanabe, Jun-ichi and Sakon, Itsuki and Shimonishi, Takashi and Onaka, Takashi},
