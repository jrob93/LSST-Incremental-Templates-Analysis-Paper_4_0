  shorttitle = {{{TNO}} or {{Comet}}?},
  author = {Bufanda, Erica and Meech, Karen J. and Kleyna, Jan T. and Hainaut, Olivier R. and Bauer, James M. and Stephens, Haynes and Veres, Peter and Micheli, Marco and Keane, Jacqueline V. and Weryk, Robert and Wainscoat, Richard and Sahu, Devendra K. and Bhatt, Bhuwan C.},
