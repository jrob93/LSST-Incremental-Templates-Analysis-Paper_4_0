@article{okamotoMonteCarloSimulation2022,
  title = {Monte {{Carlo Simulation}} of {{Dust Particles}} in a {{Protoplanetary Disk}}: {{Crystalline}} to {{Amorphous Silicate Ratio}} in {{Comets}}},
  shorttitle = {Monte {{Carlo Simulation}} of {{Dust Particles}} in a {{Protoplanetary Disk}}},
