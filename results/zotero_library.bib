  author = {Levison, Harold F. and Marchi, Simone and Noll, Keith S. and Spencer, John R. and Statler, Thomas S. and Bell, James F. and Bierhaus, Edward B. and Binzel, Richard and Bottke, William F. and Britt, Daniel and Brown, Michael E. and Buie, Marc W. and Christensen, Philip R. and Dello Russo, Neil and Emery, Joshua P. and Grundy, William M. and Hahn, Matthias and Hamilton, Victoria E. and Howett, Carly and Kaplan, Hannah and Kretke, Katherine and Lauer, Tod R. and Manzoni, Claudia and Marschall, Raphael and Martin, Audrey C. and May, Brian H. and Mottola, Stefano and Olkin, Catherine B. and P{\"a}tzold, Martin and Parker, Joel Wm and Porter, Simon and Preusker, Frank and Protopapa, Silvia and Reuter, Dennis C. and Robbins, Stuart J. and Salmon, Julien and Simon, Amy A. and Stern, S. Alan and Sunshine, Jessica M. and Wong, Ian and Weaver, Harold A. and Adam, Coralie and Ancheta, Shanti and Andrews, John and Anwar, Saadat and Barnouin, Olivier S. and Beasley, Matthew and Berry, Kevin E. and Birath, Emma and Bolin, Bryce and Booco, Mark and Burns, Rich and Campbell, Pam and Carpenter, Russell and Crombie, Katherine and Effertz, Mark and Eifert, Emily and Ellis, Caroline and Faiks, Preston and Fischetti, Joel and Fleming, Paul and Francis, Kristen and Franco, Ray and Freund, Sandy and Gallagher, Claire and Geeraert, Jeroen and Gobat, Caden and Gorgas, Donovan and Granat, Chris and Gray, Sheila and Haas, Patrick and Harch, Ann and Hegedus, Katie and Isabelle, Chris and Jackson, Bill and Jacob, Taylor and Jennings, Sherry and Kaufmann, David and Keeney, Brian A. and Kennedy, Thomas and Lauffer, Karl and {Lessac-Chenen}, Erik and Leonard, Rob and Levine, Andrew and Lunsford, Allen and Martin, Tim and McAdams, Jim and Mehall, Greg and Merkley, Trevor and Miller, Graham and Montanaro, Matthew and Montgomery, Anna and Murphy, Graham and Myers, Maxwell and Nelson, Derek S. and Ocampo, Adriana and Olds, Ryan and Pelgrift, John Y. and Perkins, Trevor and Pineau, Jon and Poland, Devin and Ramanan, Vaishnavi and Rose, Debi and Sahr, Eric and Short, Owen and Solanki, Ishita and Stanbridge, Dale and Sutter, Brian and Talpas, Zachary and Taylor, Howard and Treiu, Bo and Vermeer, Nate and Vincent, Michael and Wallace, Mike and Weigle, Gerald and Wibben, Daniel R. and Wiens, Zach and Wilson, John P. and Zhao, Yifan},
  year = {2024},
  month = may,
  journal = {Nature},
  volume = {629},
  number = {8014},
  pages = {1015--1020},
  publisher = {Nature Publishing Group},
  issn = {1476-4687},
  doi = {10.1038/s41586-024-07378-0},
  urldate = {2024-06-20},
  abstract = {Asteroids with diameters less than about 5\,km have complex histories because they are small enough for radiative torques (that is, YORP, short for the Yarkovsky--O'Keefe--Radzievskii--Paddack effect)1 to be a notable factor in their evolution2. (152830) Dinkinesh is a small asteroid orbiting the Sun near the inner edge of the main asteroid belt with a heliocentric semimajor axis of 2.19\,au; its S-type spectrum3,4 is typical of bodies in this part of the main belt5. Here we report observations by the Lucy spacecraft6,7 as it passed within 431\,km of Dinkinesh. Lucy revealed Dinkinesh, which has an effective diameter of only 720\,m, to be unexpectedly complex. Of particular note is the presence of a prominent longitudinal trough overlain by a substantial equatorial ridge and the discovery of the first confirmed contact binary satellite, now named (152830) Dinkinesh I Selam. Selam consists of two near-equal-sized lobes with diameters of 210\,m and 230\,m. It orbits Dinkinesh at a distance of 3.1\,km with an orbital period of about 52.7\,h and is tidally locked. The dynamical state, angular momentum and geomorphologic observations of the system lead us to infer that the ridge and trough of Dinkinesh are probably the result of mass failure resulting from spin-up by YORP followed by the partial reaccretion of the shed material. Selam probably accreted from material shed by this event.},
  copyright = {2024 The Author(s)},
  langid = {english},
  keywords = {Asteroids,comets and Kuiper belt,Structural geology},
  file = {/Users/jrobinson/Zotero/storage/3XAZ79YP/Levison et al_2024_A contact binary satellite of the asteroid (152830) Dinkinesh.pdf}
}

