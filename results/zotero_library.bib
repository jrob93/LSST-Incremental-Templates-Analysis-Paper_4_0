  author = {Betzler, A.S. and Almeida, R.S. and Cerqueira, W.J. and Araujo, L.A. and Prazeres, C.J.M. and Jesus, J.N. and Bispo, P.A.S. and Andrade, V.B. and Freitas, Y.A.S. and Betzler, L.B.S.},
