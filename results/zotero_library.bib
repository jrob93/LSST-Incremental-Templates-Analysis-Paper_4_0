@article{seligmanSublimeOpportunityDynamics2021,
  title = {A {{Sublime Opportunity}}: {{The Dynamics}} of {{Transitioning Cometary Bodies}} and the {{Feasibility}} of \${\textbackslash}textit\{\vphantom\}{{In Situ}}\vphantom\{\}\$ {{Observations}} of {{The Evolution}} of {{Their Activity}}},
  shorttitle = {A {{Sublime Opportunity}}},
  author = {Seligman, Darryl Z. and Kratter, Kaitlin M. and Levine, W. Garrett and Jedicke, Robert},
