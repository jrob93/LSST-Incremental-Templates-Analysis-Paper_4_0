  doi = {10.3847/PSJ/ac76c1},
  urldate = {2022-08-02},
  abstract = {Previous efforts have modeled the Didymos system as two irregularly shaped rigid bodies, although it is likely that one or both components are in fact rubble piles. Here, we relax the rigid-body assumption to quantify how this affects the spin and orbital dynamics of the system following the DART impact. Given known fundamental differences between our simulation codes, we find that faster rigid-body simulations produce nearly the same result as rubble-pile models in scenarios with a moderate value for the momentum enhancement factor, \${\textbackslash}beta\$ (\${\textbackslash}beta\{{\textbackslash}sim\}3\$) and an ellipsoidal secondary. This indicates that the rigid-body approach is likely adequate for propagating the post-impact dynamics necessary to meet DART Mission requirements. Although, if Dimorphos has a highly-irregular shape or structure, or if \${\textbackslash}beta\$ is unexpectedly large, then rubble-pile effects may become important. If Dimorphos's orbit and spin state are sufficiently excited, then surface particle motion is also possible. However, these simulations are limited in their resolution and range of material parameters, so they serve as a demonstration of principle, and Future work is required to fully understand the likelihood and magnitude of surface motion.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Collisions,DART Mission},
  file = {/Users/jrobinson/Zotero/storage/J5BWZZGR/Agrusa et al_2022_Dynamical Evolution of the Didymos-Dimorphos Binary Asteroid as Rubble Piles.pdf;/Users/jrobinson/Zotero/storage/C6QFERF4/2207.html}
}

@misc{spadacciaNegativePolarizationProperties2022,
  title = {Negative Polarization Properties of Regolith Simulants -- {{Systematic}} Experimental Evaluation of Composition Effects},
  author = {Spadaccia, Stefano and Patty, C. H. Lucas and Capelo, Holly L. and Thomas, Nicolas and Pommerol, Antoine},
  year = {2022},
