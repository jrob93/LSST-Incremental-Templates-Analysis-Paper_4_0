  keywords = {Astrophysics - Earth and Planetary Astrophysics,N-Body,Solar System Dynamics},
  file = {/Users/jrobinson/Zotero/storage/N5CRUR29/Gomes-Júnior et al_2022_The main perturbing objects on the orbits of (616) Prometheus and (617) Pandora.pdf;/Users/jrobinson/Zotero/storage/YP827FME/2202.html}
}

@article{knezevicSecularResonances501991,
  title = {Secular Resonances from 2 to 50 {{AU}}},
  author = {Knezevic, Z. and Milani, A. and Farinella, P. and Froeschle, {\relax Ch}. and Froeschle, {\relax Cl}.},
  year = {1991},
  month = oct,
  journal = {Icarus},
  volume = {93},
  pages = {316--330},
  issn = {0019-1035},
  doi = {10.1016/0019-1035(91)90215-F},
  urldate = {2022-02-24},
  abstract = {By means of a new algorithm which generalizes the second-order and fourth-degree secular perturbation theory of Milani and Kne{\v z}evi{\'c} (1990, Celest. Mech. 49, 347-411), we find in the a- e- I proper elements space the location of secular resonances between the precession rates of the longitudes of perihelion and node of a minor body and the corresponding eigenfrequencies of the secular perturbations of the four outer planets. Among the most interesting implications of our findings, we can quote: (i) the fact that the g = g6 (or {$\nu$}6) resonance in the inner asteroid belt lies closer than previously assumed to the densely populated Flora region, providing a plausible dynamical route to inject asteroid fragments into planet-crossing orbits; (ii) the existence of another possible meteorite source near 2.4 AU at moderate inclinations, again through g = g6; (iii) the existence, confirmed by numerical experiments, of a region affected in a chaotic way by the s = s6 (or {$\nu$}16) resonance at semimajor axis ⋍ 2.2 AU and moderate inclination, where no asteroid is observed; (iv) the possible presence of some low-inclination "rings" between the orbits of the outer planets where no major mean motion or secular resonance lies very near, allowing minor bodies to survive long times without close encounters; (v) the fact that none of the secular resonances considered in this work exists beyond 50 AU, so that these resonances cannot be effective for transporting inward comets belonging to a possible Kuiper flattened disk.},
  keywords = {Asteroid Belts,Astronomy,Gravitational Effects,Long Term Effects,N-Body,Orbital Elements,Orbital Resonances (Celestial Mechanics),Perturbation Theory,Planetary Gravitation,Resonant Dynamics,Secular Variations,Solar Orbits},
  annotation = {ADS Bibcode: 1991Icar...93..316K},
  file = {/Users/jrobinson/Zotero/storage/8BGUVJBU/Knezevic et al_1991_Secular resonances from 2 to 50 AU.pdf}
}

