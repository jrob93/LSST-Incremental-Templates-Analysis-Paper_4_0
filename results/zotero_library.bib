@article{villarroelThereBackgroundPopulation2022,
  title = {Is There a Background Population of High-Albedo Objects in Geosynchronous Orbits around {{Earth}}?},
  author = {Villarroel, Beatriz and Solano, Enrique and Guergouri, Hichem and Streblyanska, Alina and Mattsson, Lars and B{\"a}r, Rudolf E. and Mimouni, Jamal and Geier, Stefan and Gupta, Alok C. and Okororie, Vanessa and Laggoune, Khaoula and Shultz, Matthew E. and Freitas Jr., Robert A. and Ward, Martin J.},
